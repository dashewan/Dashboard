        <form id="frmImport" name="frmImport" method="post" enctype="multipart/form-data"
        action='@Url.Action("AdjustmentTableImport", "BasicData")'>
        <table>
            <tr>
                <td class="title">
                </td>
                <td>
                    <input type="file" name="FileAdjustmentTableImport" size="60" id="fileAdjustmentTableImport" />
                </td>
            </tr>
        </table>
        <div class="btn_bar" style="float: right">
            <input type="button" class="button" id="btnImportOK" value="<=OK>" />
            <input type="button" class="button" id="btnImportCancel" value="<=Cancel>" />
        </div>
        </form>
    </div>
</div>
<div id="divLock" align="center" style="display: none" class="transparent">
    <=DataProcess><br />
    <img src="../../Content/Images/Load1.gif" />
</div>
<script type="text/javascript" language="javascript">
    $(function () {
        $("#tblBasAdjustmentTable").jqGrid({
            url: '@Url.Action("GetBasAdjustmentTable", "BasicData")' + "/?AdjustmentType=" + adjustmentType,
            datatype: "json",
            mtype: 'POST',
            height: 320,
            colNames: ['<=PrintDate>', '<=TnNo>', '<=TnProperty>'],
            colModel: [
				{ width: 250, name: 'PrintDate', index: 'PrintDate' },
				{ width: 250, name: 'TnNo', index: 'TnNo' },
				{ width: 250, name: 'TnProperty', index: 'TnProperty' }
                 ],
            pager: 1,
            rowNum: 15,
            sortname: 'Serial',
            sortorder: "asc",
            rownumbers: true,
            shrinkToFit: false,
            autowidth: true,
            viewrecords: true,
            sortable: true,
            width: "100%",
            pager: $('#divPager'),
            rowList: [15, 20, 50, 100],
            imgpath: '../../Content/JS/jquery.jqGrid-3.6.4/ui/images'
        });
    });
</script>
