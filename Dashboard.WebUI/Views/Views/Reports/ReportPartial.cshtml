            var url = '../../Upload/AdjustmentTable Template.xlsx';
            window.location.href = url;
        });

        $("#btnImportCancel").click(function () {
            $("#dlgImport").dialog("close");
        });

        $("#btnImportOK").click(function () {
            if ($("#fileAdjustmentTableImport").val() == "") {
                alert("<=FirstSelectUpload>");
                return;
            }
            var url = '@Url.Action("AdjustmentTableImport", "BasicData")';
            url += "/?AdjustmentType=" + adjustmentType;
            $('#frmImport').attr('action', url);
            PageLock();
            $('#frmImport').ajaxSubmit({
                success: function (result) {
                    PageUnLock();
                    alert(result);
                    searchData();
                    $("#btnImportCancel").click();
                },
                error: function () {
                    PageUnLock();
                }
            })
        });
    });
</script>
