<script type="text/javascript">
    $(function () {
        $("#txtShipper").focus(function () {
            $("#dlgShipper").dialog("open");
            var url = '@Url.Action("GetCustInfo", "MasterData")';
            url += "/?CustAttribute=0";
            $('#tblLeftShipper').setGridParam({ url: url, page: 1 }).trigger("reloadGrid");
            $("#tblRightShipper").clearGridData();
        });

        $("#btnLeftShipper").click(function () {
            var gridLeftShipper = $("#tblLeftShipper");
            var gridRightShipper = $("#tblRightShipper");
            var id = gridRightShipper.jqGrid('getGridParam', 'selarrrow');
            if (id.length < 1) {
                return;
            }
            var arr = new Array();
            for (var i = 0; i < id.length; i++) {
                arr.push(id[i]);
            }
            $.each(arr, function (i, v) {
                gridRightShipper.delRowData(v);
            });
        });

        $("#btnRightShipper").click(function () {
            var gridLeftShipper = $("#tblLeftShipper");
            var gridRightShipper = $("#tblRightShipper");
            var id = gridLeftShipper.jqGrid('getGridParam', 'selarrrow');
            if (id.length < 1) {
                return;
            }
            var arr = new Array();
            for (var i = 0; i < id.length; i++) {
                arr.push(id[i]);
            }
            $.each(arr, function (i, v) {
                var leftCustCode = gridLeftShipper.getCell(v, 'CustCode');
                arrIds = gridRightShipper.getDataIDs();
                var exist = false;
                if (arrIds.length > 0) {
                    $.each(arrIds, function (b, s) {
                        var rightCustCode = gridRightShipper.getCell(s, 'CustCode');
                        if (rightCustCode == leftCustCode) {
                            exist = true;
                            return;
                        }
                    });
                }
                if (!exist) {
                    var rows = gridRightShipper.find('tr').length;
                    var newRowData = gridLeftShipper.getRowData(v);
                    gridRightShipper.addRowData(rows, newRowData);
                }
            });
        });

        $("#btnShipperOk").click(function () {
            var strShipper = "";
            var gridRightShipper = $("#tblRightShipper");
            var arrIds = gridRightShipper.getDataIDs();
            $.each(arrIds, function (b, s) {
                var shipper = $('#tblRightShipper').getCell(s, 'CustCode');
                strShipper = strShipper + shipper + ",";
            });
            $("#txtShipper").val(strShipper);
            $("#dlgShipper").dialog("close");
        });

        $("#btnShipperCancel").click(function () {
            $("#dlgShipper").dialog("close");
        });

        $("#btnShipperSearch").click(function () {
            var url = '@Url.Action("GetCustInfo", "MasterData")';
            url += "/?CustAttribute=0";
            url += "&Shipper=" + escape($("#txShipperSearch").val());
            $('#tblLeftShipper').setGridParam({ url: url, page: 1 }).trigger("reloadGrid");
        });
    })
</script>
